{
  "name": "updateAPI",
  "description": "update API",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "copy/background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs", "management", "storage"],
  "content_scripts": [
    {
      "matches": ["*://yapi.miguatech.com/*"],
      "js": ["content-script.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [{
    "resources": ["copy/aomi-yapi.js", "copy/prettier.mjs", "copy/ts.mjs", "copy/custom.js"],
    "matches": ["*://*/*"]
  }],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}
